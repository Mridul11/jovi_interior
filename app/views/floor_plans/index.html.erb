<p id="notice"><%= notice %></p>


    <h1>Floor Plans</h1>

    <table class="ui celled table">
      <thead>
        <tr>
          <th>Floor plan image</th>
          <th>User</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @floor_plans.each do |floor_plan| %>
          <tr>
            <td> <img src="<%= floor_plan.floor_plan_image %>" width="50px" height="50px" ></td>
            <td><%= floor_plan.user.email %></td>
            <td><%= link_to 'Show', floor_plan %></td>
            <td><%= link_to 'Edit', edit_floor_plan_path(floor_plan) %></td>
            <td><%= link_to 'Destroy', floor_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Floor Plan', new_floor_plan_path %>
<div id="threeJsId"></div>
<hr />
<%= javascript_pack_tag "floor_plan" %>
